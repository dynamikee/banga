{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",  // L채gg till detta
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/@/*"],
      "@components/*": ["src/components/*"],  // L채gg till eventuella andra alias du anv채nder
      "@lib/*": ["src/lib/*"]                  // Exempel p책 ett annat alias
    }
  },
  "include": ["src"]
}